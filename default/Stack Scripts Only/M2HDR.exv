dialog(name: '$$$/AdobePlugin/Shared/Exposuremerge/Process/Name=Merge to HDR Pro'){	group(placement: place_row)	{		cluster(name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/SourceFiles=Source Files')		{			group(margin_top: 4, placement: place_column)			{				static_text(height: 65, horizontal: align_left, max_width: 340, multiline: true, name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/ChooseTwo=Choose two or more files from a set of exposures to merge and create a High Dynamic Range image.', vertical: align_center, view_id: '_intro', width: 320);				group(horizontal: align_left, placement: place_row)				{					group(margin_top: 3, placement: place_column, vertical: align_top)					{						static_text(name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/Use=&Use:', vertical: align_bottom);					}					group(horizontal: align_left, placement: place_column)					{						popup_menu(horizontal: align_left, resource_id: 100, view_id: '_source');						group(horizontal: align_left, placement: place_row)						{							listbox(height: 152, horizontal: align_left, item_id: 'multiselect: true', view_id: '_fileList', width: 214);							group(horizontal: align_left, placement: place_column, vertical: align_top)							{								button(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/Browse=&Browse...', view_id: '_browse');								button(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/Remove=&Remove', view_id: '_remove');								button(name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/AddOpen=Add Open &Files', view_id: '_addOpenDocs');							}						}					}				}			}			checkbox(horizontal: align_left, name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/AttemptAlign=&Attempt to Automatically Align Source Images', view_id: '_align');		}		group(horizontal: align_left, margin_top: 5, placement: place_column, vertical: align_top)		{			button(horizontal: align_fill, item_id: 'name: ok', name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/OK=OK', view_id: '_ok');			button(horizontal: align_fill, item_id: 'name: cancel', name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/Cancel=Cancel', view_id: '_cancel');		}	}}